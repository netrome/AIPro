clear
%Get files, excluding . and ..
files = dir('../../results');
files = files(3:end);

for file=files';
   data = csvread(strcat('../../results/',file.name));
   %Extract the interesting parts of the filename
   expression = 'A-(?<a>[^-_]*)_N-(?<n>\d*)_S-(?<w>\d*)x(?<h>\d*)_E-(?<e>\d*\.\d*).csv','names';
   meta = regexp(file.name,expression,'names');
   
   figure()
   hold on
   title(strcat(meta.a,' with ',meta.n,' agents on ','meta.w',))
   plot(data(:,1), data(:,3));
end